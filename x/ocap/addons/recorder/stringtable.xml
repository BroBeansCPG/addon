<?xml version="1.0" encoding="utf-8" ?>
<Project name="OCAP_recorder">
  <Package name="Settings">
    <Container name="All">
      <Key ID="STR_OCAP_recorder_SettingsAutoStart">
        <Original>Auto-Start Settings</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_AutoStartRecording">
        <Original>Auto Start Recording</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_AutoStartRecording_Tooltip">
        <Original>Automatically start OCAP recordings at session start. Default: true</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MinimumPlayerCount">
        <Original>Minimum Player Count</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MinimumPlayerCount_Tooltip">
        <Original>Auto-start will begin once this player count is reached. Default: 15</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_SettingsCore">
        <Original>Core Settings</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_FrameCaptureDelay">
        <Original>Frame Capture Delay</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_FrameCaptureDelay_Tooltip">
        <Original>Positioning, medical status, and crew states of units and vehicles will be captured every X number of seconds. Default: 1</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_UseACE3Medical">
        <Original>Use ACE3 Medical</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_UseACE3Medical_Tooltip">
        <Original>If true, will check ACE3 medical status on units. If false, or ACE3 isn't loaded, fall back to vanilla. Default: true</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_SettingsExclusions">
        <Original>Exclusion Settings</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_ClassnamesToExclude">
        <Original>Classnames to Exclude</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_ClassnamesToExclude_Tooltip">
        <Original>Array of object classnames that should be excluded from recording. Use single quotes! Default: ['ACE_friesAnchorBar']</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_KindOfToExclude">
        <Original>Object KindOfs to Exclude</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_KindOfToExclude_Tooltip">
        <Original>Array of classnames which, along with all child classes, should be excluded from recording. Use single quotes! Default: ['WeaponHolder']</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MarkerPrefixesExclude">
        <Original>Marker Prefixes to Exclude</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MarkerPrefixesExclude_Tooltip">
        <Original>Array of prefixes. Any marker NAMES matching these prefixes will be excluded from recording. Use single quotes! Default: ['SystemMarker_','ACE_BFT_']</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_SettingsExtraTracking">
        <Original>Extra Tracking Settings</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_TicketTracking">
        <Original>Enable Ticket Tracking</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_TicketTracking_Tooltip">
        <Original>Will track respawn ticket counts for missionNamespace and each playable faction every 30th frame. Default: true</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MissionTimeTracking">
        <Original>Enable Mission Time Tracking</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MissionTimeTracking_Tooltip">
        <Original>Will continuously track in-game world time during a mission. Useful for accelerated/skipped time scenarios. Default: false</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MissionTimeTrackingInterval">
        <Original>Mission Time Tracking Interval</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MissionTimeTrackingInterval_Tooltip">
        <Original>If time tracking is enabled, it will be checked every X capture frames. Default: 10</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_SettingsSaveExport">
        <Original>Save/Export Settings</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MissionTag">
        <Original>Mission Tag</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MissionTag_Tooltip">
        <Original>If not overriden by the exportData CBA event or if a mission is auto-saved, this will be used to categorize and filter the recording in the database and web list of missions. Default: "TvT"</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_SaveOnEnd">
        <Original>Auto-save on MPEnded Event</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_SaveOnEnd_Tooltip">
        <Original>If true, automatically save and export the mission when the MPEnded event fires. Default: true</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_SaveOnEmpty">
        <Original>Auto-Save When No Players</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_SaveOnEmpty_Tooltip">
        <Original>Will automatically save recording when there are 0 players on the server and existing data accounts for more time than the minimum save duration setting. Default: true</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MinimumSaveDuration">
        <Original>Minimum Save Duration</Original>
      </Key>
      <Key ID="STR_OCAP_recorder_MinimumSaveDuration_Tooltip">
        <Original>A recording must be at least this long (in minutes) to auto-save. Calling an 'ocap_exportData' CBA server event will override this restriction. Default: 20</Original>
      </Key>
    </Container>
  </Package>

